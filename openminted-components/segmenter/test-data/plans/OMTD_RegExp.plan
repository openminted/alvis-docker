<alvisnlp-plan id="OMTD_ToMap">
  <param name="input">
    <alias module="read" param="source"/>
  </param>

  <param name="pattern">
    <alias module="regexp" param="pattern"/>
  </param>

  <read class="XMIImport"/>

  <regexp class="RegExp">
    <targetLayerName>regexp</targetLayerName>
  </regexp>
  
  <write class="XMIExport">
    <outDir>.</outDir>
  </write>
</alvisnlp-plan>
